          "range": false,
          "refId": "A"
        }
      ],
      "title": "API Endpoints Status",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Value": true
            },
            "indexByName": {
              "endpoint": 0,
              "status_code": 1
            },
            "renameByName": {
              "endpoint": "Endpoint",
              "status_code": "Status Code"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Description",
            "mode": "valueMapping",
            "reduce": {
              "include": ["Status Code"]
            },
            "mappings": [
              {"type": "value", "value": "200", "text": "Success"},
              {"type": "value", "value": "201", "text": "Created"},
              {"type": "value", "value": "400", "text": "Bad Request"},
              {"type": "value", "value": "401", "text": "Unauthorized"},
              {"type": "value", "value": "404", "text": "Not Found"},
              {"type": "value", "value": "415", "text": "Unsupported Media Type"},
              {"type": "value", "value": "500", "text": "Server Error"}
            ]
          }
        }
